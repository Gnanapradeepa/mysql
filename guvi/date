select curdate();
SELECT SUBDATE("2018-01-22", INTERVAL 2 MONTH);
SELECT DATE_FORMAT(CURDATE(), '%d/%m/%Y');

SELECT DATE_FORMAT(curdate(),  '%b%d,%Y');
select str_to_date(concat(12,31,extract(year from curdate())),'%m%d%Y');
